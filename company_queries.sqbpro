--SQL-Business-Analytics-Project(SQLite)
-- Using CTE for window functions
-- ------------------------------------------

-- 1️⃣ Employees with salary, highest first
SELECT e.emp_id, e.name, s.salary
FROM employees e
JOIN salaries s ON e.emp_id = s.emp_id
ORDER BY s.salary DESC;

-- 2️⃣ Rank employees by salary across company
WITH ranked AS (
    SELECT e.emp_id, e.name, s.salary,
           RANK() OVER (ORDER BY s.salary DESC) AS salary_rank
    FROM employees e
    JOIN salaries s ON e.emp_id = s.emp_id
)
SELECT *
FROM ranked
ORDER BY salary_rank;

-- 3️⃣ Rank employees within each department
WITH dept_ranked AS (
    SELECT e.emp_id, e.name, d.dept_name, s.salary,
           RANK() OVER (PARTITION BY e.dept_id ORDER BY s.salary DESC) AS dept_salary_rank
    FROM employees e
    JOIN salaries s ON e.emp_id = s.emp_id
    JOIN departments d ON e.dept_id = d.dept_id
)
SELECT *
FROM dept_ranked
ORDER BY dept_name, dept_salary_rank;

-- 4️⃣ Average salary per department for each employee
WITH dept_avg AS (
    SELECT e.emp_id, e.name, d.dept_name, s.salary,
           AVG(s.salary) OVER (PARTITION BY e.dept_id) AS dept_avg_salary
    FROM employees e
    JOIN salaries s ON e.emp_id = s.emp_id
    JOIN departments d ON e.dept_id = d.dept_id
)
SELECT *
FROM dept_avg
ORDER BY dept_name
LIMIT 10;

-- 5️⃣ Top 3 highest paid employees per department
WITH ranked AS (
    SELECT e.emp_id, e.name, d.dept_name, s.salary,
           RANK() OVER (PARTITION BY e.dept_id ORDER BY s.salary DESC) AS dept_rank
    FROM employees e
    JOIN salaries s ON e.emp_id = s.emp_id
    JOIN departments d ON e.dept_id = d.dept_id
)
SELECT *
FROM ranked
WHERE dept_rank &lt;= 3;

-- 6️⃣ Number of employees per department
SELECT d.dept_name, COUNT(e.emp_id) AS num_employees
FROM employees e
JOIN departments d ON e.dept_id = d.dept_id
GROUP BY d.dept_name
ORDER BY num_employees DESC;

-- 7️⃣ Average and maximum salary per department
SELECT d.dept_name, AVG(s.salary) AS avg_salary, MAX(s.salary) AS max_salary
FROM employees e
JOIN salaries s ON e.emp_id = s.emp_id
JOIN departments d ON e.dept_id = d.dept_id
GROUP BY d.dept_name
ORDER BY avg_salary DESC;

-- 8️⃣ Employees with more than 2 projects
SELECT e.emp_id, e.name, COUNT(ep.project_id) AS num_projects
FROM employees e
JOIN employee_projects ep ON e.emp_id = ep.emp_id
GROUP BY e.emp_id, e.name
HAVING num_projects &gt; 2
ORDER BY num_projects DESC;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
